<main>

	<div class = "tableClassSets">

		<div><h2>Schedual Sets</h2></div>



	{{#unless classSet}}
		<p>No Schedual Sets</p>
	{{/unless}}



	{{#if classSet }}
		<div class= "tableHolder">
			<table>
				<thread>
				<tr>
					<th>NAME</th>
					<th>Credit Count</th>
					<th>Term</th>
				</tr>
				</thread>
				<tbody>

				{{#each classSet}}

					{{> classSets}}
				{{/each}}

				</tbody>
			</table>
		</div>

	<div>
	{{/if}}
		<div class = "addButtonHolder">
			<a href = "/edit"><button id="addButton" class="action-button">New Set</button></a>
		</div>

	</div>

</main>
